#box {
  width: 100%;
  height: 100vh;
  background-color: #ffffff;
  border-radius: 14px;
  position: fixed;
  top: 45vw;
  box-shadow: 0 -5px 5px rgba(0, 0, 0, 0.5);
  padding: 10vw 5vw;
  box-sizing: border-box;
  z-index: 2;
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-template-rows: 50vw 50vw;
}
.choose{
  width: 40vw;
  height: 40vw;
  border-radius: 20px;
  /* position: fixed; */
  padding: 20px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
  margin:2.5vw;
  box-sizing: border-box;
  z-index: 2;
}
