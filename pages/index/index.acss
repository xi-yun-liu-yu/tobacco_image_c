/*以下遮罩*/
.container {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.overlay {
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5); 
  z-index: 2;
}

.loading-container {
  position: fixed;
  top: 42%;
  left: 44%;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 3; 

}

.loading {
  border: 4px solid #f3f3f3; 
  border-top: 4px solid rgb(48,195,190); 
  border-radius: 50%;
  width: 40px;
  height: 40px;
  animation: spin 2s linear infinite;
}

@keyframes spin {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}

.lode-txt{
  position: absolute;
  top: 49%;
  left: 42%;
}

/*以上遮罩*/

.table_container {
  width: 100%;
  border-collapse: collapse; /* 边框重叠合并 */
  padding: 40rpx 40rpx, 40rpx, 40rpx; /* 上下20rpx, 左右40rpx的内边距 */
  max-height: 70vh;
}
