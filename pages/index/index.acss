.history_bt {
  background-color: rgb(48,195,190); /* 蓝色背景 */
  color: #fff; /* 白色文字 */
  border: none; /* 无边框 */
  border-radius: 15rpx; /* 圆角 */
  font-size: 5vw; /* 文字大小 */
  padding: 0rpx 40rpx; /* 上下20rpx, 左右40rpx的内边距 */
  box-shadow: 0 4rpx 10rpx rgba(0,0,0,0.1); /* 阴影效果 */  
  font-family: "仿宋";
  font-weight: 600;
}

.img_container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 100%;
  padding-bottom: 5vw;
  /* padding-top: 5vw; */
}

.button_container {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  padding-top: 30px;
  width: 100%;
}

.uploaded-img {
  border-radius: 0; /* 如果不需要圆角，可以设置为0 */
  box-shadow: none; /* 如果不需要阴影，可以移除 */
}

#unShow{
  display: none;
}

